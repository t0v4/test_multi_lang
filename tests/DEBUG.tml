### "Initialization of variables";nwl;
tce;
var SUBTRACT_FACTOR 10;nwl;
var SCREEN_WIDTH 0;nwl;
var SCREEN_HEIGHT 0;nwl;
var CELL_WIDTH 2;nwl;
var CELL_HEIGHT 2;nwl;
var SCREEN_BUFFER "";nwl;
mlt CELL_WIDTH 10 SCREEN_WIDTH;nwl;
mlt CELL_HEIGHT 10 SCREEN_HEIGHT;nwl;
sub SCREEN_WIDTH SUBTRACT_FACTOR SCREEN_WIDTH;nwl;
sub SCREEN_HEIGHT SUBTRACT_FACTOR SCREEN_HEIGHT;nwl;
var CURRENT_X 0;nwl;
var CURRENT_Y 0;nwl;
tce;
### "Beginning of render script";nwl;
whl "CURRENT_Y < SCREEN_HEIGHT";nwl;idn 1;
whl "CURRENT_X < SCREEN_WIDTH";nwl;idn 2;
mlt "#" CELL_WIDTH CELL_CHAR;nwl;idn 2;
add SCREEN_BUFFER CELL_CHAR SCREEN_BUFFER;nwl;idn 2;
add CURRENT_X 1 CURRENT_X;nwl;idn 1;
wrt SCREEN_BUFFER;nwl;idn 1;
var SCREEN_BUFFER "";nwl;idn 1;
var CURRENT_X 0;nwl;idn 1;
add CURRENT_Y 1 CURRENT_Y;nwl;nwl;END;